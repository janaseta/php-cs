{
    "name": "janaseta/php-cs",
    "description": "Jāņa sēta's php-cs-fixer config",
    "type": "library",
    "authors": [
        {
            "name": "Juris Evertovskis",
            "email": "juris.evertovskis@kartes.lv"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "JanaSeta\\PhpCs\\": "src"
        }
    },
    "scripts": {
		"cs": "vendor/bin/php-cs-fixer fix --dry-run --diff",
		"fix": "vendor/bin/php-cs-fixer fix"
    }
}
