{
    "name": "janaseta/php-cs",
    "description": "Jāņa sēta's php-cs-fixer config",
    "type": "library",
    "authors": [
        {
            "name": "Juris Evertovskis",
            "email": "juris.evertovskis@kartes.lv"
        }
    ],
    "require": {
        "php": "^8.1",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "JanaSeta\\PhpCs\\": "src"
        }
    },
    "scripts": {
		"fix": "php-cs-fixer fix",
		"cs": "@fix --dry-run --diff"
    },
    "scripts-descriptions": {
        "fix": "Fix code style",
		"cs": "Dry run CS fixer"
    }
}
